<?xml version="1.0" encoding="UTF-8"?>
<image xmlns:ext="http://exist-db.org/vra/extension" xmlns="http://www.vraweb.org/vracore4.htm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="$IMAGE_ID$" href="$IMAGE_Href$" refid="$IMAGE_Filename$">
    <agentSet>
        <display>$IMAGE_AgentDisplay$</display>
        <agent>
            <name type="$IMAGE_Agent1NameType$" vocab="$IMAGE_Agent1NameVocab$" refid="$IMAGE_Agent1NameRefid$">$IMAGE_Agent1Name$</name>
            <role vocab="$WORK_Agent1RoleVocab$" refid="$WORK_Agent1RoleRefid$">$IMAGE_Agent1Role$</role>
        </agent>
    </agentSet>
    <dateSet>
        <display>$IMAGE_DateDisplay$</display>
        <date/>
    </dateSet>
    <descriptionSet>
        <display>$IMAGE_Description$</display>
        <description>
            <text>$IMAGE_Description$</text>
        </description>
    </descriptionSet>
    <inscriptionSet>
        <display>$IMAGE_InscriptionDisplay$</display>
        <inscription>
            <text>$IMAGE_InscriptionDisplay$</text>
        </inscription>
    </inscriptionSet>
    <locationSet>
        <display>$IMAGE_LocationDisplay$</display>
        <notes>$IMAGE_LocationNotes$</notes>
        <location type="repository">
            <name>$IMAGE_LocalRepoName$</name>
            <refid type="accession">$IMAGE_AccessionID$</refid>
        </location>
        <location type="$IMAGE_LocationType$">
            <name type="$IMAGE_LocationNameType$" vocab="$IMAGE_LocationNameVocab$" refid="$IMAGE_LocationNameRefID$">$IMAGE_LocationName$</name>
            <refid type="$IMAGE_LocationObjType$">$IMAGE_LocationObjRefID$</refid>
            <name type="geographic" vocab="$IMAGE_LocationGeoVocab$" refid="$IMAGE_LocationGeoRefid$">$IMAGE_LocationGeo$</name>
            <point epsg="$IMAGE_LocationGeoPointEPSG$">
                <lat>$IMAGE_LocationGeoPointLat$</lat>
                <long>$IMAGE_LocationGeoPointLong$</long>
            </point>
        </location>
    </locationSet>
    <materialSet>
        <display>$IMAGE_MaterialDisplay$</display>
        <material/>
    </materialSet>
    <measurementsSet>
        <display>$IMAGE_MeasurementsDisplay$</display>
        <measurements/>
    </measurementsSet>
    <relationSet>
        <display>$IMAGE_RelationDisplay$</display>
        <relation type="imageOf" source="$SourceOfImageRecord$" relids="$WORK_ID$"/>
    </relationSet>
    <rightsSet>
        <display>$IMAGE_RightsDisplay$</display>
        <notes>$IMAGE_RigthsNotes$</notes>
        <rights type="$IMAGE_RigthsType$">
            <rightsHolder>$IMAGE_RigthsHolder$</rightsHolder>
            <text>$IMAGE_RigthsText$</text>
        </rights>
    </rightsSet>
    <sourceSet>
        <display>$IMAGE_SourceDisplay$</display>
        <source>
            <name type="vendor">$IMAGE_VendorName$</name>
            <refid type="vendor">$IMAGE_VendorID$</refid>
        </source>
        <source>
            <name type="$IMAGE_SourceType$">$IMAGE_SourceValue$</name>
        </source>
    </sourceSet>
    <subjectSet>
        <display>$IMAGE_SubjectDisplay$</display>
        <subject>
            <term type="$WORK_SubjectType1$" vocab="$WORK_SubjectVocab1$" refid="$WORK_SubjectRefid1$">$WORK_Subject1$</term>
        </subject>
        <subject>
            <term type="$WORK_SubjectType2$" vocab="$WORK_SubjectVocab2$" refid="$WORK_SubjectRefid2$">$WORK_Subject2$</term>
        </subject>
        <subject>
            <term type="$WORK_SubjectType3$" vocab="$WORK_SubjectVocab3$" refid="$WORK_SubjectRefid3$">$WORK_Subject3$</term>
        </subject>
    </subjectSet>
    <techniqueSet>
        <display>$IMAGE_TechniqueDisplay$</display>
        <technique vocab="AAT" refid="300237903">digital imaging</technique>
    </techniqueSet>
    <titleSet>
        <display>$IMAGE_TitleDisplay$</display>
        <title type="$IMAGE_TitleType$">$IMAGE_Title$</title>
    </titleSet>
    <worktypeSet>
        <display>$IMAGE_WorktypeDisplay$</display>
        <worktype vocab="AAT" refid="300215302">digital images</worktype>
    </worktypeSet>
</image>