<?xml version="1.0" encoding="UTF-8"?>
<image xmlns:ext="http://exist-db.org/vra/extension" xmlns="http://www.vraweb.org/vracore4.htm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="$IMAGE_ID$" href="$IMAGE_Href$" refid="$IMAGE_Filename$">
    <agentSet>
        <display>$IMAGE_AgentDisplay$</display>
        <agent>
            <name vocab="$IMAGE_Agent1NameVocab$" refid="$IMAGE_Agent1NameRefid$">$IMAGE_Agent1Name$</name>
            <role>$IMAGE_Agent1Role$</role>
        </agent>
    </agentSet>
    <dateSet>
        <display>$IMAGE_DateDisplay$</display>
        <date/>
    </dateSet>
    <descriptionSet>
        <display>$IMAGE_Description$</display>
        <description>$IMAGE_Description$</description>
    </descriptionSet>
    <inscriptionSet>
        <display>$IMAGE_InscriptionDisplay$</display>
        <inscription>
            <text>$IMAGE_InscriptionDisplay$</text>
        </inscription>
    </inscriptionSet>
    <locationSet>
        <display>$IMAGE_LocationDisplay$</display>
        <location type="repository">
            <name>$IMAGE_LocalRepoName$</name>
            <refid type="accession">$IMAGE_AccessionID$</refid>
        </location>
    </locationSet>
    <materialSet>
        <display>$IMAGE_MaterialDisplay$</display>
        <material/>
    </materialSet>
    <measurementsSet>
        <display>$IMAGE_MeasurementsDisplay$</display>
        <measurements/>
    </measurementsSet>
    <relationSet>
        <display>$IMAGE_RelationDisplay$</display>
        <relation type="imageOf" relids="$WORK_ID$">$WORK_TitlePref$</relation>
    </relationSet>
    <rightsSet>
        <display>$IMAGE_RightsDisplay$</display>
        <notes>$IMAGE_RigthsNotes$</notes>
        <rights type="$IMAGE_RigthsType$">
            <rightsHolder>$IMAGE_RigthsHolder$</rightsHolder>
            <text>$IMAGE_RigthsText$</text>
        </rights>
    </rightsSet>
    <sourceSet>
        <display>$IMAGE_SourceDisplay$</display>
        <source>
            <name type="vendor">$IMAGE_VendorName$</name>
            <refid type="vendor">$IMAGE_VendorID$</refid>
        </source>
        <source>
            <name type="$IMAGE_SourceType$">$IMAGE_SourceValue$</name>
        </source>
    </sourceSet>
    <subjectSet>
        <display>$IMAGE_SubjectDisplay$</display>
        <subject>
            <term type="$IMAGE_SubjectType1$" vocab="$IMAGE_SubjectVocab1$" refid="$IMAGE_SubjectRefid1$">$IMAGE_Subject1$</term>
            <term type="$IMAGE_SubjectType2$" vocab="$IMAGE_SubjectVocab2$" refid="$IMAGE_SubjectRefid2$">$IMAGE_Subject2$</term>
            <term type="$IMAGE_SubjectType3$" vocab="$IMAGE_SubjectVocab3$" refid="$IMAGE_SubjectRefid3$">$IMAGE_Subject3$</term>
        </subject>
    </subjectSet>
    <techniqueSet>
        <display>$IMAGE_TechniqueDisplay$</display>
        <technique vocab="$IMAGE_TechniqueVocab$" refid="$IMAGE_TechniqueRefid$">$IMAGE_Technique$</technique>
    </techniqueSet>
    <titleSet>
        <display>$IMAGE_TitleDisplay$</display>
        <title type="$IMAGE_TitleType$">$IMAGE_Title$</title>
    </titleSet>
    <worktypeSet>
        <display>$IMAGE_WorktypeDisplay$</display>
        <worktype vocab="$IMAGE_WorktypeVocab$" refid="$IMAGE_WorktypeRefid$">$IMAGE_Worktype$</worktype>
    </worktypeSet>
</image>